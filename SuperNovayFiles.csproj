<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="yWorks.yFilesWPF.DevelopmentLicense.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="yWorks.yFilesWPF.DevelopmentLicense.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="yWorks.yFilesWPF.Adapter" Version="3.6.0" />
    <PackageReference Include="yWorks.yFilesWPF.Algorithms" Version="3.6.0" />
    <PackageReference Include="yWorks.yFilesWPF.Viewer" Version="3.6.0" />
  </ItemGroup>

</Project>
